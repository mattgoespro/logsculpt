{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "debug.console.collapseIdenticalLines": true,
  "debug.console.fontSize": 12,
  "debug.console.lineHeight": -0.1,
  "debug.console.wordWrap": true,
  "debug.showInStatusBar": "always",
  "debug.toolBarLocation": "commandCenter",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "editor.defaultFoldingRangeProvider": "esbenp.prettier-vscode",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.enable": true,
  "eslint.format.enable": false,
  "eslint.probe": ["javascript", "typescript"],
  "eslint.run": "onType",
  "eslint.useFlatConfig": true,
  "eslint.validate": ["javascript", "typescript"],
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    ".eslintignore": "ignore"
  },
  "files.eol": "\n",
  "files.exclude": {
    "**/node_modules/**": true
  },
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/.vscode/**": true,
    "**/node_modules/**": true
  },
  "files.trimTrailingWhitespace": true,
  "javascript.referencesCodeLens.enabled": true,
  "javascript.referencesCodeLens.showOnAllFunctions": true,
  "javascript.suggest.completeFunctionCalls": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jest.debugMode": true,
  "jest.nodeEnv": {
    "NODE_OPTIONS": "--experimental-vm-modules --disable-warning=ExperimentalWarning"
  },
  "npm-intellisense.scanDevDependencies": true,
  "npm-outdated.majorUpdateProtection": false,
  "prettier.configPath": "prettier.config.cjs",
  "search.exclude": {
    "**/dist": true,
    "**/node_modules": true,
    "**/package-lock.json": true
  },
  "terminal.integrated.automationProfile.windows": {
    "args": ["-l"],
    "color": "terminal.ansiMagenta",
    "env": {
      "PATH": "${env:PATH}",
      "NODE_OPTIONS": "--experimental-vm-modules --disable-warning=ExperimentalWarning"
    },
    "icon": "terminal-debian",
    "overrideName": true,
    "path": "C:\\Program Files\\Git\\bin\\bash.exe"
  },
  "terminal.integrated.defaultProfile.windows": "bash-node",
  "terminal.integrated.profiles.windows": {
    "bash-node": {
      "args": ["-l"],
      "color": "terminal.ansiGreen",
      "icon": "terminal-bash",
      "overrideName": true,
      "path": "C:\\Program Files\\Git\\bin\\bash.exe",
      "env": {
        "PATH": "${env:PATH}",
        "NODE_OPTIONS": "--experimental-vm-modules --disable-warning=ExperimentalWarning"
      }
    }
  },
  "testing.automaticallyOpenPeekViewDuringAutoRun": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.tsdk": "node_modules\\typescript\\lib",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.iconTheme": "material-icon-theme"
}
